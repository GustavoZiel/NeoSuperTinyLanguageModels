            "syntactic": [
                {
                    "prompt": "On {BIRTH_DATE}, in {CITY}, {NAME}",
                    "completion": " was born"
                },
                {
                    "prompt": "{COLOR} is the favourite color of",
                    "completion": " {NAME}"
                },
                {
                    "prompt": "{NAME}'s social security number is",
                    "completion": " {SSN}"
                },
                {
                    "prompt": "At {COMPANY}, {NAME} works as a",
                    "completion": " {PROFESSION}"
                },
                {
                    "prompt": "The owner of the {ITEM} is",
                    "completion": " {NAME}"
                },
                {
                    "prompt": "With {FLUENCY} fluency, {NAME} speaks",
                    "completion": " {LANGUAGE}"
                }
            ],
            "semantic": [
                {
                    "prompt": "The date and city of birth of {NAME} are",
                    "completion": " {BIRTH_DATE} and {CITY}"
                },
                {
                    "prompt": "{NAME}'s preferred color is",
                    "completion": " {COLOR}"
                },
                {
                    "prompt": "The social security number of {NAME} is",
                    "completion": " {SSN}"
                },
                {
                    "prompt": "The employer of {NAME} is",
                    "completion": " {COMPANY}"
                },
                {
                    "prompt": "The {ITEM} belongs to",
                    "completion": " {NAME}"
                },
                {
                    "prompt": "{NAME} is a {FLUENCY} speaker of the",
                    "completion": " {LANGUAGE} language"
                }
            ]
        }
    },
